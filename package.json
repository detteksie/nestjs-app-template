{
  "name": "nest-app-template",
  "version": "1.0.0",
  "description": "Nest Application Template",
  "author": "S Abd Malik",
  "license": "UNLICENSED",
  "main": "dist/main",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:dist": "cross-env NODE_ENV=production env-cmd node .",
    "start:prod": "node .",
    "start:pm2": "pm2-runtime ecosystem.config.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test jest --maxWorkers=1",
    "test:watch": "cross-env NODE_ENV=test jest --watch --maxWorkers=1",
    "test:cov": "cross-env NODE_ENV=test jest --coverage --maxWorkers=1",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.config.js --detectOpenHandles --maxWorkers=1",
    "torm": "typeorm-ts-node-commonjs -d dataSource.js",
    "migration:generate": "npm run torm -- migration:generate -p -o",
    "migration:run": "npm run torm -- migration:run",
    "migration:run:prod": "cross-env NODE_ENV=production npm run torm -- migration:run",
    "migration:revert": "npm run torm -- migration:revert",
    "migration:revert:prod": "cross-env NODE_ENV=production npm run torm -- migration:revert",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/mapped-types": "^2.0.0",
    "@nestjs/mongoose": "^10.0.6",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.0.0",
    "@nestjs/throttler": "^5.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "bcrypt": "^5.1.1",
    "cache-manager": "^5.5.1",
    "cache-manager-ioredis-yet": "^2.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.0.0",
    "helmet": "^7.1.0",
    "husky": "^8.0.0",
    "mongodb": "^6.0.0",
    "mongoose": "^8.3.2",
    "multer": "1.4.4-lts.1",
    "nanoid": "^3.3.6",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.11.5",
    "pm2": "^5.3.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.0.0",
    "@types/jest": "^29.5.0",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.0.0",
    "@types/passport-jwt": "^4.0.0",
    "@types/passport-local": "^1.0.0",
    "@types/pg": "^8.11.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "keywords": [],
  "lint-staged": {
    "*.{md,html,css,json,js,ts}": "prettier -w",
    "*.{js,ts}": "eslint --cache --fix"
  }
}
